PORT=3000
NODE_ENV=development

POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=myapp

DATABASE_URL="postgresql://user:password@postgres:5432/myapp"

# Logging Configuration
LOG_LEVEL=debug              # Logging level: debug, info, warn, error (default: debug in dev, info in prod)
LOG_DB_QUERIES=true          # Enable database query logging (default: true in dev, false in prod)